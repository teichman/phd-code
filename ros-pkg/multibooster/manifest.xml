<package>
  <description brief="multibooster">

    MultiBooster is a multi-class, multi-descriptor boosting algorithm which uses hypersphere weak classifiers.

  </description>
  <author>Alex Teichman</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/multibooster</url>
  <depend package="eigen"/>
  <depend package="pipeline"/>
  <depend package="qpOASES"/>
  <depend package="name_mapping"/>
  <depend package="performance_statistics"/>
  <depend package="eigen_extensions"/>
  <depend package="optimization"/>
  <depend package="bag_of_tricks"/>
  
  <export>
    <cpp cflags="-I${prefix}/include" lflags="-L${prefix}/lib -Wl,-rpath ${prefix}/lib -lmultibooster"/>
  </export>

</package>


