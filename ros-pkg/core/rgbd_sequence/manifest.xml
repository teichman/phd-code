<package>
  <description brief="rgbd_sequence">

     rgbd_sequence

  </description>
  <author>teichman</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/rgbd_sequence</url>
  <!-- pcl_bleeding has to come before anything that causes inclusion of /opt/ros/fuerte/include -->
  <depend package="pcl_bleeding"/>
  <depend package="serializable"/>
  <depend package="opencv_bleeding"/>
  <depend package="bag_of_tricks"/>
  <!-- depend package="sensor_msgs" /-->
  <depend package="roscpp"/>
  <depend package="eigen_extensions"/>
  <depend package="google-perftools"/>
  <depend package="optimization"/>

  <export>
    <cpp cflags="-I${prefix}/include"
	 lflags="-Wl,-rpath,${prefix}/lib -L${prefix}/lib -lrgbd_sequence `rosboost-cfg --lflags filesystem`" />
  </export>
  
</package>


